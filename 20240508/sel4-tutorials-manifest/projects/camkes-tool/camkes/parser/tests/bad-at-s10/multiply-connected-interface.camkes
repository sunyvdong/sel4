connector Conn {
    from Procedure;
    to Procedure;
}

procedure P {
    void foo();
}

component Foo {
    uses P p;
    uses P q;
}

component Bar {
    provides P p;
}

assembly {
    composition {
        component Foo f;
        component Bar b;
        connection Conn c1(from f.p, to b.p);
        connection Conn c2(from f.q, to b.p);
    }
}
